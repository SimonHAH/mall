<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cskaoyan.mall.mapper.systemMapper.OptionsMapper">
    <resultMap id="BaseResultMap" type="com.cskaoyan.mall.bean.system.Options">
        <result column="id"  property="value" />
        <result column="name"  property="label" />
    </resultMap>

<select id="selectCount" resultMap="BaseResultMap">
    select
      id as `value`,
      `name` as label
    from
      cskaoyan_mall_role
</select>
</mapper>